
 <%= search_form_for(@q) do |f| %>
  <div class="sidemenu-panel">
    <div class="panel panel-default">
     <div class="panel-heading">
       検索
     </div>
     <div class="panel-body">
       <%= f.text_field :shopname_cont, id: "input02", class: "form-control", placeholder: "【店名】" %>
     </div>
     <div class="panel-heading">
       ジャンル
     </div>
     <div class="panel-body">
       <%= f.label :category_eq, 'ジャンル' %>
       <%= f.select :category_eq, [["指定なし", ""],["中華", "中華"], ["ラーメン", "ラーメン"], ["居酒屋", "居酒屋"], ["イタリアン", "イタリアン"], ["和食", "和食"], ["喫茶店", "喫茶店"], ["その他", "その他"]] ,class:"form-control" %>
     </div>
     <div class="panel-heading">
       時間
     </div>
     <div class="panel-body">
     <p class="downstarttime">
        <%= f.label :starttime_eq, '時間' %>
        <%= f.select :starttime_gteq, [["指定なし", ""],["10:00", "10:00"], ["10:30", "10:30"], ["11:00", "11:00"], ["11:30", "11:30"], ["12:00", "12:00"], ["12:30", "12:30"], ["13:00", "13:00"], ["13:30", "13:30"], ["14:00", "14:00"], ["14:30", "14:30"], ["15:00", "15:00"],["16:00", "16:00"], ["16:30", "16:30"], ["17:00", "17:00"], ["17:30", "17:30"], ["18:00", "18:00"], ["18:30", "18:30"], ["19:00", "19:00"], ["19:30", "19:30"], ["20:00", "20:00"], ["20:30", "20:30"], ["21:00", "21:00"], ["21:30","21:30"], ["22:00", "22:00"], ["22:30", "22:30"], ["23:00", "23:00"], ["23:30", "23:30"], ["24:00", "24:00"]] ,class:"form-control" %>
     </p>
     </div>
     <div class="panel-heading">
       予算
     </div>
     <div class="panel-body">
       <div class="starttime-kensaku">
        <%= f.select :charge_gteq, [["指定なし", ""],["¥1.000", 1000], ["¥2.000", 2000], ["¥3.000", 3000], ["¥4.000", 4000], ["¥5.000", 5000], ["¥6.000", 6000], ["¥7.000", 7000], ["¥8.000", 8000], ["¥9.000", 9000], ["¥10.000", 10000], ["¥15.000", 15000],["¥20.000", 20000], ["¥25.000", 25000], ["¥30.000", 30000], ["¥40.000", 40000], ["¥50.000", 50000]]%>
       </div>
       <%= f.label :charge_lteq, "〜" %>
       <div class="starttime-kensaku">
        <%= f.select :charge_lteq, [["指定なし", ""],["¥1.000", 1000], ["¥2.000", 2000], ["¥3.000", 3000], ["¥4.000", 4000], ["¥5.000", 5000], ["¥6.000", 6000], ["¥7.000", 7000], ["¥8.000", 8000], ["¥9.000", 9000], ["¥10.000", 10000], ["¥15.000", 15000],["¥20.000", 20000], ["¥25.000", 25000], ["¥30.000", 30000], ["¥40.000", 40000], ["¥50.000", 50000]]%>
       </div>
     </div>
     <div class="panel-heading">
       営業時間
     </div>
     <div class="panel-body">
         <%= radio_button_tag :starttime,1, checked: true %>
           0時
         <%= radio_button_tag :starttime,2 %>
           6時以前
         <%= radio_button_tag :starttime,3 %>
           12時以前
         <%= radio_button_tag :starttime,4 %>
           18時以降
     </div>
     <div class="panel-heading">
       営業時間
     </div>
     <div class="panel-body">
       <div class="starttime-kensaku">
         <%= f.time_field :starttime_gteq %>
       </div>
         <%= f.label :price_lteq, "〜" %>
       <div class="finishtime-kensaku">
         <%= f.time_field :finishtime_lteq %>
       </div>
     </div>
  </div>
</div>
<% end %>
